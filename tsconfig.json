{
  "compilerOptions": {
    "target": "ES6",
    "moduleResolution": "node",
    "ignore": [
      "node_modules",
      "packages"
    ],
    "esModuleInterop": true,
    "noResolve": false,
    "noImplicitAny": false,
    "preserveConstEnums":true,
    "lib": [
      "dom",
      "es6"
    ],
    "rootDir": "src",
  }
}